<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme.PagerProject" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/redpink</item>
        <item name="colorSecondary">@color/medyowhite</item>
        <item name="colorAccent">@color/DarkGray</item>
        <item name="android:statusBarColor">?android:attr/statusBarColor</item>
    </style>
    <style name="Theme.PagerProject.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="DialogAnimationShrink">
        <item name="android:windowEnterAnimation">@anim/animate_shrink_enter</item>
        <item name="android:windowExitAnimation">@anim/animate_shrink_exit</item>
    </style>
<!--    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />-->

<!--    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />-->
</resources>